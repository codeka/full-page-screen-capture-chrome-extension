{
  "name": "Screen Capture",
  "version": "2.1.0",
  "manifest_version": 2,
  "description": "Screen capture parts of your page, or the whole page in it's entirety, reliably!",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "menu.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+P"
      }
    }
  },
  "permissions": [ "activeTab" ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
